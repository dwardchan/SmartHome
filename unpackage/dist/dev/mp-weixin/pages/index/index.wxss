  url('https://at.alicdn.com/t/font_2190622_u9xdue8x2j.woff2') format('woff2'),
  url('https://at.alicdn.com/t/font_2190622_u9xdue8x2j.woff') format('woff'),
  url('https://at.alicdn.com/t/font_2190622_u9xdue8x2j.ttf') format('truetype'),
  url('https://at.alicdn.com/t/font_2190622_u9xdue8x2j.svg#iconfont') format('svg');
